
CREATE TABLE `users` (
	`ID` VARCHAR(50) NULL DEFAULT NULL,
	`PW` VARCHAR(50) NULL DEFAULT NULL,
	`NICKNAME` VARCHAR(100) NULL DEFAULT NULL,
	`EMAIL` VARCHAR(200) NULL DEFAULT NULL,
	`NOTE` TEXT NULL DEFAULT NULL,
	`PER_CODE` VARCHAR(50) NULL DEFAULT NULL
)
COMMENT='사용자정보들어가기'
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;


CREATE TABLE `permission` (
	`PER_CODE` VARCHAR(50) NULL DEFAULT NULL,
	`PER_NAME` VARCHAR(200) NULL DEFAULT NULL,
	`PER_AREA` VARCHAR(100) NULL DEFAULT NULL,
	`NOTE` TEXT NULL DEFAULT NULL
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;




CREATE TABLE `notice_board` (
	`BOARD_SEQ` INT(11) NOT NULL COMMENT '게시판순서',
	`TITLE` VARCHAR(200) NOT NULL COMMENT '제목',
	`INFORMATION` TEXT NULL DEFAULT NULL COMMENT '내용',
	`FILE_CODE` VARCHAR(20) NULL DEFAULT NULL COMMENT '첨부파일',
	`INSERT_USER_ID` VARCHAR(50) NULL DEFAULT NULL COMMENT '작성자ID',
	`INSERT_DATE` DATE NULL DEFAULT NULL COMMENT '작성일'
)
COMMENT='게시판테이블'
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

